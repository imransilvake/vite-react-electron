{
	"name": "robot-gui",
	"private": true,
	"version": "0.2.1",
	"main": "./electron/output/main/index.js",
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:ci": "CI=true vitest",
		"pretty:check": "prettier --check './**/*.{js,jsx,ts,tsx,json}'",
		"pretty:apply": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
		"lint:es": "npx eslint './**/*.{js,jsx,ts,tsx}' --quiet",
		"lint:es:fix": "npx eslint './**/*.{js,jsx,ts,tsx}' --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es && yarn test:ci",
		"release": "release-it",
		"release:major": "yarn release-it -- major --ci",
		"release:minor": "yarn release-it -- minor --ci",
		"release:patch": "yarn release-it -- patch --ci",
		"electron:start": "electron-vite dev",
		"electron:preview": "electron-vite preview",
		"electron:build": "electron-vite build"
	},
	"dependencies": {
		"@electron-toolkit/preload": "^1.0.3",
		"@electron-toolkit/utils": "^1.0.2",
		"@fontsource/open-sans": "^4.5.14",
		"dayjs": "^1.11.7",
		"i18next": "^22.4.13",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^12.2.0",
		"sass": "^1.60.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.5.1",
		"@commitlint/config-conventional": "^17.4.4",
		"@release-it/conventional-changelog": "^5.1.1",
		"@types/node": "^18.15.11",
		"@types/react": "^18.0.31",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"@typescript-eslint/parser": "^5.57.0",
		"@vitejs/plugin-react": "^3.1.0",
		"electron": "^23.2.0",
		"electron-vite": "^1.0.21",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"husky": "^8.0.3",
		"prettier": "^2.8.7",
		"release-it": "^15.9.3",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vitest": "^0.29.8"
	}
}
